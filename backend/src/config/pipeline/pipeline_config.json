{
  "defaults": {
    "chunking": {
      "chunk_size": 1000,
      "overlap": 200
    },
    "embedding": {
      "model": "voyage-multilingual-2",
      "dimensions": 1024,
      "batch_size": 100,
      "provider": "voyage"
    },
    "retrieval": {
      "method": "hybrid",
      "top_k": 5,
      "similarity_threshold": 0.7,
      "similarity_metric": "cosine"
    },
    "generation": {
      "model": "google/gemini-2.5-flash-lite",
      "temperature": 0.1,
      "max_tokens": 1000
    },
    "orchestration": {
      "max_concurrent_documents": 5,
      "step_timeout_minutes": 30
    }
  },
  "indexing": {
    "partition": {
      "hybrid_mode": true,
      "scanned_detection": {
        "text_threshold": 25,
        "sample_pages": 3
      },
      "ocr_strategy": "auto",
      "extract_tables": true,
      "extract_images": true,
      "max_image_size_mb": 10,
      "ocr_languages": [
        "dan"
      ],
      "table_validation": {
        "enabled": true,
        "max_table_size": 5000,
        "max_columns": 20,
        "max_cells": 100,
        "repetition_threshold": 0.7,
        "min_confidence": 0.3,
        "reject_on_drawing_pages": true
      }
    },
    "metadata": {
      "extract_page_structure": true,
      "detect_sections": true,
      "preserve_formatting": true
    },
    "enrichment": {
      "merge_related_elements": true,
      "min_content_length": 50
    },
    "chunking": {
      "chunk_size": 1000,
      "overlap": 200,
      "strategy": "semantic",
      "separators": [
        "\n\n",
        "\n",
        " ",
        ""
      ],
      "min_chunk_size": 100,
      "max_chunk_size": 1500,
      "include_section_titles": false
    },
    "embedding": {
      "model": "voyage-multilingual-2",
      "provider": "voyage",
      "dimensions": 1024,
      "batch_size": 100,
      "max_retries": 3,
      "retry_delay": 1.0,
      "timeout_seconds": 30,
      "cost_tracking": true,
      "resume_capability": true
    },
    "storage": {
      "collection_prefix": "construction_docs",
      "validation_sample_size": 50
    },
    "orchestration": {
      "max_concurrent_documents": 10,
      "step_timeout_minutes": 30,
      "retry_attempts": 3,
      "fail_fast": true
    }
  },
  "query": {
    "query_processing": {
      "provider": "openrouter",
      "model": "openai/gpt-3.5-turbo",
      "fallback_models": [
        "anthropic/claude-3-haiku"
      ],
      "timeout_seconds": 1.0,
      "max_tokens": 200,
      "temperature": 0.1,
      "variations": {
        "semantic_expansion": false,
        "hyde_document": false,
        "formal_variation": false,
        "parallel_generation": false
      }
    },
    "retrieval": {
      "embedding_model": "voyage-multilingual-2",
      "dimensions": 1024,
      "similarity_metric": "cosine",
      "top_k": 5,
      "similarity_thresholds": {
        "excellent": 0.05,
        "good": 0.04,
        "acceptable": 0.03,
        "minimum": 0.01
      },
      "danish_thresholds": {
        "excellent": 0.05,
        "good": 0.04,
        "acceptable": 0.03,
        "minimum": 0.01
      }
    },
    "generation": {
      "provider": "openrouter",
      "model": "google/gemini-2.5-flash-lite",
      "fallback_models": [
        "anthropic/claude-3.5-haiku",
        "meta-llama/llama-3.1-8b-instruct"
      ],
      "timeout_seconds": 5.0,
      "max_tokens": 1000,
      "temperature": 0.1,
      "response_format": {
        "include_citations": true,
        "include_confidence": true,
        "language": "danish"
      }
    },
    "quality_analysis": {
      "enable_automatic_metrics": true,
      "enable_user_feedback": true,
      "quality_thresholds": {
        "excellent": 0.8,
        "good": 0.6,
        "acceptable": 0.4,
        "poor": 0.2
      },
      "monitoring": {
        "enable_dashboard": true,
        "retention_days": 30,
        "alert_threshold": 0.5
      }
    }
  },
  "wiki": {
    "generation": {
      "temperature": 0.2,
      "max_pages": 4,
      "queries_per_page": 4
    },
    "retrieval": {
      "top_k": 10,
      "max_chunks_per_page": 20,
      "similarity_threshold": 0.15,
      "embedding_model": "voyage-multilingual-2",
      "dimensions": 1024
    }
  }
}